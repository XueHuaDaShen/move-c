{
  "extends": "airbnb",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-indent": ["error", 4],
    "comma-dangle": 0,
    "eol-last": "off",
    "react/jsx-filename-extension": 0
  }
}